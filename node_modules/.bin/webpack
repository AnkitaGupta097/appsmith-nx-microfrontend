#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jaij/check/nx-rspack-microfrontend-demo/node_modules/webpack/bin/node_modules:/home/jaij/check/nx-rspack-microfrontend-demo/node_modules/webpack/node_modules:/home/jaij/check/nx-rspack-microfrontend-demo/node_modules:/home/jaij/check/node_modules:/home/jaij/node_modules:/home/node_modules:/node_modules:/home/jaij/check/nx-rspack-microfrontend-demo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jaij/check/nx-rspack-microfrontend-demo/node_modules/webpack/bin/node_modules:/home/jaij/check/nx-rspack-microfrontend-demo/node_modules/webpack/node_modules:/home/jaij/check/nx-rspack-microfrontend-demo/node_modules:/home/jaij/check/node_modules:/home/jaij/node_modules:/home/node_modules:/node_modules:/home/jaij/check/nx-rspack-microfrontend-demo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../webpack/bin/webpack.js" "$@"
fi
