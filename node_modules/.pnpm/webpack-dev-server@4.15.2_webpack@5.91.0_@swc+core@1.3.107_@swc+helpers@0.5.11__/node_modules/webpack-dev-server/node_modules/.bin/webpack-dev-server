#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jaij/check/nx-rspack-microfrontend-demo/node_modules/.pnpm/webpack-dev-server@4.15.2_webpack@5.91.0_@swc+core@1.3.107_@swc+helpers@0.5.11__/node_modules/webpack-dev-server/bin/node_modules:/home/jaij/check/nx-rspack-microfrontend-demo/node_modules/.pnpm/webpack-dev-server@4.15.2_webpack@5.91.0_@swc+core@1.3.107_@swc+helpers@0.5.11__/node_modules/webpack-dev-server/node_modules:/home/jaij/check/nx-rspack-microfrontend-demo/node_modules/.pnpm/webpack-dev-server@4.15.2_webpack@5.91.0_@swc+core@1.3.107_@swc+helpers@0.5.11__/node_modules:/home/jaij/check/nx-rspack-microfrontend-demo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jaij/check/nx-rspack-microfrontend-demo/node_modules/.pnpm/webpack-dev-server@4.15.2_webpack@5.91.0_@swc+core@1.3.107_@swc+helpers@0.5.11__/node_modules/webpack-dev-server/bin/node_modules:/home/jaij/check/nx-rspack-microfrontend-demo/node_modules/.pnpm/webpack-dev-server@4.15.2_webpack@5.91.0_@swc+core@1.3.107_@swc+helpers@0.5.11__/node_modules/webpack-dev-server/node_modules:/home/jaij/check/nx-rspack-microfrontend-demo/node_modules/.pnpm/webpack-dev-server@4.15.2_webpack@5.91.0_@swc+core@1.3.107_@swc+helpers@0.5.11__/node_modules:/home/jaij/check/nx-rspack-microfrontend-demo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../../bin/webpack-dev-server.js" "$@"
fi
