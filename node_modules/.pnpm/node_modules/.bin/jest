#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jaij/check/nx-rspack-microfrontend-demo/node_modules/.pnpm/jest-cli@29.7.0_@types+node@18.14.2_ts-node@10.9.1_@swc+core@1.3.107_@swc+helpers@0.5.11__@ty_arqape6qrycnekzxdoivt76vzm/node_modules/jest-cli/bin/node_modules:/home/jaij/check/nx-rspack-microfrontend-demo/node_modules/.pnpm/jest-cli@29.7.0_@types+node@18.14.2_ts-node@10.9.1_@swc+core@1.3.107_@swc+helpers@0.5.11__@ty_arqape6qrycnekzxdoivt76vzm/node_modules/jest-cli/node_modules:/home/jaij/check/nx-rspack-microfrontend-demo/node_modules/.pnpm/jest-cli@29.7.0_@types+node@18.14.2_ts-node@10.9.1_@swc+core@1.3.107_@swc+helpers@0.5.11__@ty_arqape6qrycnekzxdoivt76vzm/node_modules:/home/jaij/check/nx-rspack-microfrontend-demo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jaij/check/nx-rspack-microfrontend-demo/node_modules/.pnpm/jest-cli@29.7.0_@types+node@18.14.2_ts-node@10.9.1_@swc+core@1.3.107_@swc+helpers@0.5.11__@ty_arqape6qrycnekzxdoivt76vzm/node_modules/jest-cli/bin/node_modules:/home/jaij/check/nx-rspack-microfrontend-demo/node_modules/.pnpm/jest-cli@29.7.0_@types+node@18.14.2_ts-node@10.9.1_@swc+core@1.3.107_@swc+helpers@0.5.11__@ty_arqape6qrycnekzxdoivt76vzm/node_modules/jest-cli/node_modules:/home/jaij/check/nx-rspack-microfrontend-demo/node_modules/.pnpm/jest-cli@29.7.0_@types+node@18.14.2_ts-node@10.9.1_@swc+core@1.3.107_@swc+helpers@0.5.11__@ty_arqape6qrycnekzxdoivt76vzm/node_modules:/home/jaij/check/nx-rspack-microfrontend-demo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest-cli/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest-cli/bin/jest.js" "$@"
fi
